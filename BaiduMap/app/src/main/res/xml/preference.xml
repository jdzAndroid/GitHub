        <Preference
            android:enabled="true"
            android:key="abort"
            android:icon="@drawable/set_resetmap"
            android:title="@string/set_aboart"
            ></Preference>



        <Preference
            android:enabled="true"
            android:key="newfunction"
            android:icon="@drawable/set_newfunction"
            android:title="@string/set_newfunction"
            ></Preference>

        <Preference
            android:enabled="true"
            android:key="apply"
            android:icon="@drawable/set_apply"
            android:title="@string/set_apply"
            ></Preference>

    </PreferenceCategory>
</PreferenceScreen>