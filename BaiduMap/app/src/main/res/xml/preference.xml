<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <PreferenceCategory
        android:enabled="true"
        android:key="setting"
        android:title="@string/setting"
        android:icon="@drawable/go_back"
        >
        <CheckBoxPreference
            android:enabled="true"
            android:key="set_screen_light"
            android:id="@+id/screen_light"
            android:title="@string/set_screenlight"
            android:icon="@drawable/set_screenlight"
            >
        </CheckBoxPreference>
        <CheckBoxPreference
            android:enabled="true"
            android:key="set_showImg"
            android:id="@+id/screen_show_image"
            android:title="@string/set_isshowimage"
            android:icon="@drawable/set_showimg"
            >

        </CheckBoxPreference>


        <CheckBoxPreference
            android:enabled="true"
            android:key="set_showMsg"
            android:id="@+id/screen_message_show"
            android:title="@string/set_messageshow"
            android:icon="@drawable/set_showmeg"
